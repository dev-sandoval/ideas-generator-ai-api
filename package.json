{
  "name": "ideas-generator-ai-api",
  "version": "1.0.0",
  "description": "API para generar ideas de publicaciones para negocios usando IA",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "start": "bun src/index.ts",
    "build": "bun build src/index.ts --outdir ./dist --target bun",
    "typecheck": "bunx tsc --noEmit",
    "lint": "bunx eslint src --ext .ts"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.1",
    "@google/genai": "^1.40.0",
    "elysia": "^1.4.24",
    "openai": "^6.21.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^24.10.13",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "keywords": [
    "api",
    "backend",
    "ai",
    "business",
    "ideas",
    "typescript",
    "bun",
    "elysia"
  ],
  "author": "David Sandoval | @sandovaldavid",
  "license": "MIT"
}
